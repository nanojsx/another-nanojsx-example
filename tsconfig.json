{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",

    "jsx": "react-jsx",
    "jsxImportSource": "nano-jsx/esm",

    "target": "ES2020",
    "module": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
